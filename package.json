{
  "name": "fluentapp",
  "description": "mono repo starter projects",
  "private": true,
  "workspaces": [
    "packages/*",
    "apps/*"
  ],
  "scripts": {
    "docs": "doctoc --title '**Table of content**' README.md",
    "clean": "pnpm run -r clean",
    "build": "pnpm run -r build",
    "test": "pnpm run -r test",
    "lint": "eslint --ext js,ts,tsx .",
    "format": "prettier . --write",
    "serve": "set NODE_ENV=development&&gulp --require sucrase/register/ts -f scripts/gulpserve.ts",
    "serve:main": "pnpm run -C main serve",
    "serve:main-antd": "pnpm run -C main-antd serve",
    "serve:system": "pnpm run serve -C packages/system",
    "serve:article": "pnpm run serve -C packages/article",
    "serve:links": "pnpm run serve -C packages/links",
    "clean:dist": "rimraf dist",
    "build:main": "pnpm run -C main build",
    "build:main-antd": "pnpm run -C main-antd build",
    "build:system": "pnpm run build -C packages/system",
    "build:article": "pnpm run build -C packages/article",
    "build:links": "pnpm run build -C packages/links",
    "preinstall": "npx only-allow pnpm -y",
    "check:git-pristine": "./scripts/check-git-pristine.sh",
    "clean:global-cache": "rimraf ./.cache",
    "clean:turbo": "rimraf --glob .cache/turbo '**/.turbo' '**/turbo-*.log' && turbo daemon clean",
    "deps:check": "npx npm-check-updates@latest --configFileName ncurc.yml --workspaces --root --mergeConfig",
    "deps:update": "npx npm-check-updates@latest --configFileName ncurc.yml -u --workspaces --root --mergeConfig",
    "g:build": "turbo run build --cache-dir=.cache/turbo",
    "g:changeset": "changeset",
    "g:check-dist": "turbo run check-dist --cache-dir=.cache/turbo",
    "g:clean": "yarn clean:global-cache && yarn clean:turbo && rimraf ./out ",
    "g:codegen": "turbo run codegen --cache-dir=.cache/turbo",
    "g:fix-all-files": "turbo run fix-all-files",
    "g:lint": "TIMING=1 turbo run lint --cache-dir=.cache/turbo --color",
    "g:lint-staged-files": "lint-staged --allow-empty",
    "g:lint-styles": "turbo run lint-styles --cache-dir=.cache/turbo --color",
    "g:release": "yarn g:build && changeset publish",
    "g:test-e2e": "turbo run test-e2e --cache-dir=.cache/turbo",
    "g:test-unit": "turbo run test-unit --cache-dir=.cache/turbo",
    "g:typecheck": "turbo run typecheck --cache-dir=.cache/turbo",
    "g:turbo": "turbo",
    "install:playwright": "playwright install",
    "install:husky": "husky install",
    "nuke:node_modules": "rimraf --glob '**/node_modules'",
    "postinstall": "run-s install:husky g:codegen"
  },
  "devDependencies": {
    "@types/fs-extra": "^11.0.4",
    "@types/gulp": "^4.0.17",
    "chalk": "5.3.0",
    "doctoc": "~2.2.1",
    "dotenv": "^16.4.1",
    "dotenv-expand": "^10.0.0",
    "eslint": "~8.56.0",
    "eslint-plugin-import": "~2.29.1",
    "fs-extra": "^11.2.0",
    "rimraf": "^5.0.5",
    "sucrase": "^3.35.0",
    "typescript": "~5.3.3",
    "@changesets/changelog-github": "0.5.0",
    "@changesets/cli": "2.27.1",
    "@commitlint/cli": "18.6.0",
    "@commitlint/config-conventional": "18.6.0",
    "@types/shell-quote": "1.7.5",
    "husky": "9.0.10",
    "lint-staged": "15.2.2",
    "npm-run-all2": "6.1.2",
    "prettier": "3.2.5",
    "shell-quote": "1.8.1",
    "syncpack": "12.3.0",
    "turbo": "1.12.2",
    "vitest": "^1.2.2",
    "format": "^0.2.2"
  },
  "dependencies": {
    "cross-env": "7.0.3",
    "qiankun": "^2.10.16"
  },
  "peerDependencies": {
    "qiankun": "^2.6.3"
  }
}
